    "@nestjs/platform-socket.io": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/websockets": "^9.0.0",
    "@prisma/client": "^4.12.0",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.1.0",
